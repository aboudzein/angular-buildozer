    "ng2-smart-table": "^1.5.0",
    "ngx-gravatar": "^8.0.0",
    "ngx-infinite-scroll": "^8.0.0",
    "ngx-perfect-scrollbar": "^8.0.0",
    "rxjs": "~6.4.0",
    "semi-table": "0.0.5",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
